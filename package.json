{
  "name": "pivot-editor",
  "description": "A simple open source utility for editing Odoo pipeline pivot table definitions (osheet.json files) with a modern UI.",
  "author": "Paul Sems <paulsems@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently --kill-others --success first \"npm:dev\" \"wait-on http://localhost:5173 && electron .\"",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^38.1.1",
    "electron-builder": "^24.13.3",
    "vite": "^7.1.5",
    "wait-on": "^7.2.0"
  }
}
